{
    "name": "wowangjuanzi/migrate",
    "description": "Database migration tool by illuminate component",
    "keywords": [
        "migrate",
        "migration",
        "illuminate",
        "laravel",
        "lumen"
    ],
    "authors": [
        {
            "name": "mowangjuanzi",
            "email": "baoguoxiao0538@hotmail.com",
            "homepage": "https://www.baoguoxiao.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "baoguoxiao0538@hotmail.com",
        "issues": "https://github.com/mowangjuanzi/migrate/issues"
    },
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "illuminate/filesystem": "^6.5",
        "illuminate/events": "^6.5",
        "illuminate/console": "^6.5",
        "illuminate/config": "^6.5",
        "illuminate/database": "^6.5",
        "vlucas/phpdotenv": "^3.6"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3.4"
    },
    "autoload": {
        "files": [
            "helper.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php -r \"file_exists('artisan') || symlink('console.php', 'artisan');\""
        ]
    }
}
